---
import Contacto from "./contacto.astro"

const CONTACT =[
  {
    title:"Contacto",
    label: "contacto",
    url: "/#Contacto",
  }
]
---
{CONTACT.map (link =>(
    <a
  class="inline-block rounded bg-teal-500 px-8 py-3 text-sm font-medium text-white transition hover:scale-110 hover:shadow-xl focus:outline-none focus:ring active:bg-teal-500"
  href={link.url}
>
  {link.title}
</a>
))}